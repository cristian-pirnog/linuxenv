StrictHostKeyChecking ask

# tunnel approach, ssh -N -f tunnel, then ssh cluster_tunnel
Host tunnel
        HostName 10.194.42.10
        User cpirnog
        LocalForward 12366 login.hpc.cam.ac.uk:22
        IdentityFile ~/.ssh/id_rsa

Host cluster_tunnel
        HostName localhost
        Port 12366
        User co-vite4
        IdentityFile ~/.ssh/cluster_rsa

# Proxy approach, does not require a tunnel to be set up, just use ssh cluser
Host cluster
        HostName login.hpc.cam.ac.uk
        User co-vite4
        PreferredAuthentications publickey
        ProxyCommand ssh -i  ~/.ssh/id_rsa cpirnog@10.194.42.10 -W %h:%p
#        IdentityFile ~/.ssh/cluster_rsa

Host cluster2
       HostName oscgate.oerc.ox.ac.uk
       User gtirenni
       PreferredAuthentications publickey
       ProxyCommand ssh -i  ~/.ssh/id_rsa cpirnog@10.194.42.10 -W %h:%p 2> /dev/null
#       IdentityFile ~/.ssh/cluster_rsa

