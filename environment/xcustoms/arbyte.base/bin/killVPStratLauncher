#!/bin/bash

# Note: cmd must stay at the end, such that it is printed in full
processIds=`ps -e -opid,ruser:20,cmd|grep " ${USER} " |grep './VPStratLauncher' | grep ' --stratname ArByte' | grep -v 'grep ' | awk '{print $1}'`

if [[ -z ${processIds} ]]; then
    echo "No processes found to kill"
    exit 0
fi

echo "Killing process with id: ${processIds}"
kill $@ ${processIds}
