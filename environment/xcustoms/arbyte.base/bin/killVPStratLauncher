#!/bin/bash

processIds=`ps -eo pid,cmd,ruser|grep " ${USER}$" |grep './VPStratLauncher' | grep ' --stratname ArByte' | grep -v 'grep ' | awk '{print $1}'`

if [[ -z ${processIds} ]]; then
    echo "No processes found to kill"
    exit 0
fi

echo "Killing process with id: ${processIds}"
kill $@ ${processIds}
