  "file-icons": {}
  "go-plus":
    currentPanelHeight: "9.03072033898305vh"
  "haskell-ghc-mod": {}
  linter: {}
  "linter-ui-default":
    showPanel: true
  "tree-view": {}
  welcome:
    showOnStartup: false
".apiary.source":
  editor:
    tabLength: 4
".apib.gfm.html.markdown.source.text":
