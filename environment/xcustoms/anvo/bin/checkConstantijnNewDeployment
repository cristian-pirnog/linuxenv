#!/bin/bash

# The directory where the repository checkout for tests on constantijn resides
MATLAB_CONSTANTIJN=$HOME'/data/main/fromsvn/matlab/matlab_constantijn'


# Go to the correct directory...
cd $MATLAB_CONSTANTIJN

# update the svn repository
svn update

# ... and run matlab with DQ_App_TestConstantijn
matlab -r DQ_App_TestConstantijn

